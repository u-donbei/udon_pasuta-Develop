<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<BorderPane fx:id="mainPane" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <top>
      <ImageView fx:id="logo" fitHeight="150.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true" BorderPane.alignment="CENTER">
         <image>
            <Image url="@../../../../../../../../../../../../../../AppData/Roaming/うどんべいのパスタ退治/Images/logo.png" />
         </image>
      </ImageView>
   </top>
   <center>
      <VBox fx:id="select" alignment="CENTER" prefHeight="200.0" prefWidth="100.0" BorderPane.alignment="CENTER">
         <children>
            <Label fx:id="start" text="▷Start" textFill="LIME">
               <font>
                  <Font size="30.0" />
               </font>
            </Label>
            <Label fx:id="setting" text="Setting" textFill="LIME">
               <font>
                  <Font size="30.0" />
               </font>
            </Label>
            <Label fx:id="exit" text="Exit" textFill="LIME">
               <font>
                  <Font size="30.0" />
               </font>
            </Label>
            <Label fx:id="help" text="Help" textFill="LIME">
               <font>
                  <Font size="30.0" />
               </font>
            </Label>
         </children>
      </VBox>
   </center>
</BorderPane>
